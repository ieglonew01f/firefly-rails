<p id="notice"><%= notice %></p>

<h1>Listing Online Users</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Is online</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @online_users.each do |online_user| %>
      <tr>
        <td><%= online_user.user_id %></td>
        <td><%= online_user.is_online %></td>
        <td><%= link_to 'Show', online_user %></td>
        <td><%= link_to 'Edit', edit_online_user_path(online_user) %></td>
        <td><%= link_to 'Destroy', online_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Online user', new_online_user_path %>
